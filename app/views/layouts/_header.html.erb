<header>
  <div class="content">
    <div class="left">
      <%= link_to "", root_path, class: "logo" %>
    </div>
    <div class="right">
      <% if signed_in? %>
        <%= link_to "Sign out", destroy_user_session_path, method: "delete" %>
        <%= link_to "Profile", current_user %>
        <%= link_to "Invite", invite_users_path %>
        <%= link_to "Search", users_path %>
        <%= link_to "Manage Profile", "users/#{current_user.id}/models/#{current_user.user_type.id}/edit" %>    
      <% else %>
        <%= link_to "Sign Up", new_user_registration_path, class: "buttonLink green" %>
        <%= link_to "Sign in", new_user_session_path, class: "signin" %>
      <% end %> 
    </div>    
  </div>
</header>

