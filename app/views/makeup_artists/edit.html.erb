<% provide(:title, "Edit Makeup Artist profile") %>
<h1>Makeup Artist Specific Information</h1>

<div class="row">

  <div class="span6">
    <%= form_for(@makeup_artist) do |f| %>
      <%= render 'shared/error_messages', object: @makeup_artist %>
      
      <%= f.label :model_mayhem, "URL of Your Model Mayhem Profile" %>
      <%= f.text_field :model_mayhem %>

      <%= f.label :bio %>
      <%= f.text_field :bio %>

      <%= f.label :website %>
      <%= f.text_field :website %>

      <%= f.submit "Next", class: "btn btn-large btn-primary" %>
    <% end %>

  </div>

</div>

