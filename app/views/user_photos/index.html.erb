<h1>Listing last 10 User Photo's Uploaded</h1>

<% @user_photos.each do |user_photo| %>

  <!-- link_to image_tag("path/to/image.png"), "http://www.mysite.com/" -->
  <div><%= link_to image_tag(user_photo.photo.url (:medium)), user_path(user_photo.user) %></div>
  <div><%= user_photo.caption %></div>
  <div>Uploaded: <%= user_photo.relative_time(user_photo.created_at) %></div>
  <div><%= user_photo.user.name %></div>
  <div><%= image_tag user_photo.user.get_default_photo.photo.url (:micro) %></div>

<% end %>
