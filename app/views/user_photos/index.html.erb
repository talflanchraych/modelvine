<h1>Listing ALL User Photo's Uploaded to the Site</h1>

<% @user_photos.each do |user_photo| %>

  <div><%= image_tag user_photo.photo.url %></div>
  <div>Caption: <%= user_photo.caption %></div>
  <div>User: <%= user_photo.user.name %></div>

    <%= link_to("Show", {:action => 'show', :id => user_photo.id}, :class => 'action show') %> 
    <br>

<% end %>

<%= link_to "New Photo", new_user_photo_path %>